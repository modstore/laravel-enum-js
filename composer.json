{
    "name": "modstore/laravel-enum-js",
    "description": "Package to generate javascript versions of your PHP enum files to be used in your js builds etc.",
    "keywords": [
        "modstore",
        "laravel-enum-js"
    ],
    "homepage": "https://github.com/modstore/laravel-enum-js",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Mark Whitney",
            "email": "mark@priceguard.com.au",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0|^8.1",
        "illuminate/support": ">=6.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0|^6.24|^7.6",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Modstore\\LaravelEnumJs\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Modstore\\LaravelEnumJs\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Modstore\\LaravelEnumJs\\LaravelEnumJsServiceProvider"
            ]
        }
    }
}
